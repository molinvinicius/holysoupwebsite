<style>
  .slider-nutris-hs .swiper-slide{
    width: 90%;
    height: auto;
  }
  
  .slider-nutris-hs {
    padding: 40px 20px 60px 20px;
  }

  .swiper-slide-active .nutri-hs {
    box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.01);
  }

  .nutri-hs {
    background: #fff;
    padding: 30px;
    border-radius: 25px;
    text-align: center;
    height: calc(100% - 60px);
  }

  .photo-nutri img {
    width: 100px;
    height: 100px;
    margin: 0 auto;
    object-fit: cover;
    border-radius: 9999px;
    margin-top: -50px;
    margin-bottom: 15px;
  }

  .title-nutri span {
    color: #07b;
    font-weight: 700;
    font-size: 13px;
  }
  
  .description-nutri p {
    font-size: 14px;
  }

  .avaliacao-nutri svg {
    stroke: none;
    width: 14px;
    height: auto;
  }

  .avaliacoes-head-nutri {
    display: flex;
    gap: 5px;
    align-items: center;
  }

  .avaliacoes-head-nutri .media-avaliacao p {
    font-size: 33px;
    line-height: 1;
    font-weight: 700;
    margin: 0;
  }

  .avaliacoes-head-nutri .total-avaliacoes {
    display: flex;
    flex-direction: column;
    text-align: left;
    line-height: 1;
    justify-content: center;
  }

  .head-nutri-review  .header-widget {
    display: flex;
    justify-content: space-evenly;
    width: 100%;
  }

  .head-nutri-review {
    display: flex;
    gap: 5px;
    align-items: center;
    margin-bottom: 25px;
  }

  .head-nutri-review .header-widget h3 {
    margin-left: inherit !important;
    margin-right: inherit !important;
    color: #fff;
  }

  .avaliacoes-head-nutri * {
    color: #fff;
  }

  .avaliacoes-head-nutri .star-icon svg {
    fill: #ffd88a;
    stroke: none;
    width: 16px;
    height: 16px;
    margin-top: -5px;
  }

  .nutri-hs-header{
    position: relative;
  }

