    width: 40px;
    height: 37px;
    background-size: contain;
    position: absolute;
    top: 0;
    left: 17%;
    background-repeat: no-repeat;
  }

  @media(max-width: 1024px){
    .avaliacoes-head-nutri .media-avaliacao p {
      font-size: 20px;
    }

    .avaliacoes-head-nutri .total-avaliacoes {
      font-size: 7px;
    }

    .head-nutri-review .header-widget {
      max-width: 100%;
    }
  }

  @media (min-width: 1200px){
    .slider-nutris-hs .swiper-slide {
      width: 30%;
    }
  
    .slider-nutris-hs {
      padding: 40px 120px 60px 120px;
    }
  }
</style>
{% if section.settings.borda-superior %}
  {% render 'borda-superior', cor: section.settings.color-borda-superior %}
{% endif %}
<div class="swiper slider-nutris-hs max-container">
  <div class="head-nutri-review">
      <div class="header-widget custom-font text-left">
        <h3>O que dizem os Nutricionistas</h3>
        <div class="avaliacoes-head-nutri">
          <div class="star-icon">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 50 50" width="512px" height="512px">    <path d="M10.2,48.6c-0.2,0-0.4-0.1-0.6-0.2c-0.3-0.2-0.5-0.7-0.4-1.1l4.4-16.4L0.4,20.2C0,20-0.1,19.5,0,19.1 c0.1-0.4,0.5-0.7,0.9-0.7l17-0.9l6.1-15.9C24.2,1.3,24.6,1,25,1c0.4,0,0.8,0.3,0.9,0.6l6.1,15.9l17,0.9c0.4,0,0.8,0.3,0.9,0.7 c0.1,0.4,0,0.8-0.3,1.1L36.4,30.9l4.4,16.4c0.1,0.4,0,0.8-0.4,1.1c-0.3,0.2-0.8,0.3-1.1,0L25,39.2l-14.3,9.2 C10.5,48.6,10.4,48.6,10.2,48.6z"></path></svg>
          </div>
          <div class="media-avaliacao">
            <p>4.8</p>
          </div>
          <div class="total-avaliacoes">
            <b>1.500</b>
            <span>Avaliações</span>
          </div>
        </div>
      </div>
  </div>
  <div class="swiper-wrapper">
  {% for block in section.blocks %}
    {% assign content = block.settings.title | split: ": " %}
    <div class="swiper-slide">
      <div class="nutri-hs">
        <div class="nutri-hs-header">
          <div class="photo-nutri">
            {% if block.settings.icone %}
              <img src="{{ block.settings.icone | image_url }}">
            {% endif %}
          </div>
          <div class="title-nutri">
            <span>
              {{ block.settings.title }}
            </span>
            <div class="avaliacao-nutri">
              {% assign next = 0 %}
              {% for i in (1..5) %}
                {% assign index = i | times: 1.0 %}
                {% assign next = i | times: 1.0 | plus: 1 %}
                {% assign prev = i | times: 1.0 | minus: 1 %}
                {% if block.settings.star_range >= index  %}
                  <svg xmlns="http://www.w3.org/2000/svg" width="9.727" height="9.032" viewBox="0 0 9.727 9.032">
                    <path data-name="Icon ionic-ios-star" d="M11.608,6.5H8.414l-.971-2.9a.352.352,0,0,0-.66,0L5.813,6.5H2.6a.348.348,0,0,0-.347.347.255.255,0,0,0,.007.059.334.334,0,0,0,.145.245L5.027,9,4.02,11.932a.348.348,0,0,0,.119.391.336.336,0,0,0,.2.085.426.426,0,0,0,.217-.078L7.114,10.5l2.562,1.826a.407.407,0,0,0,.217.078.312.312,0,0,0,.193-.085.344.344,0,0,0,.119-.391L9.2,9l2.6-1.867.063-.054a.332.332,0,0,0-.256-.58Z" transform="translate(-2.25 -3.375)" fill="#ffd88a"/>
                  </svg>
                {% elsif block.settings.star_range < index and block.settings.star_range > prev %}
                  <svg xmlns="http://www.w3.org/2000/svg" width="9.727" height="9.032" viewBox="0 0 9.727 9.032">
                    <path data-name="Icon ionic-ios-star-half" d="M11.608,6.5H8.414l-.971-2.9a.352.352,0,0,0-.66,0L5.813,6.5H2.6a.348.348,0,0,0-.347.347.255.255,0,0,0,.007.059.334.334,0,0,0,.145.245L5.027,9,4.02,11.932a.348.348,0,0,0,.119.391.336.336,0,0,0,.2.085.426.426,0,0,0,.217-.078L7.114,10.5l2.562,1.826a.407.407,0,0,0,.217.078.312.312,0,0,0,.193-.085.344.344,0,0,0,.119-.391L9.2,9l2.6-1.867.063-.054a.332.332,0,0,0-.256-.58Zm-1.029.764L8.846,8.51a.61.61,0,0,0-.221.69l.654,1.9a.087.087,0,0,1-.132.1l-1.681-1.2,0,0a.81.81,0,0,1-.35-.662V4.793A.042.042,0,0,1,7.2,4.78h0l.643,1.915a.607.607,0,0,0,.575.415h2.113A.087.087,0,0,1,10.579,7.266Z" transform="translate(-2.25 -3.375)" fill="#ffd88a"/>
                  </svg>
                {% else %}
                  <svg xmlns="http://www.w3.org/2000/svg" width="10.327" height="9.632" viewBox="0 0 10.327 9.632">
                    <path data-name="Icon ionic-ios-star" d="M11.608,6.5H8.414l-.971-2.9a.352.352,0,0,0-.66,0L5.813,6.5H2.6a.348.348,0,0,0-.347.347.255.255,0,0,0,.007.059.334.334,0,0,0,.145.245L5.027,9,4.02,11.932a.348.348,0,0,0,.119.391.336.336,0,0,0,.2.085.426.426,0,0,0,.217-.078L7.114,10.5l2.562,1.826a.407.407,0,0,0,.217.078.312.312,0,0,0,.193-.085.344.344,0,0,0,.119-.391L9.2,9l2.6-1.867.063-.054a.332.332,0,0,0-.256-.58Z" transform="translate(-1.95 -3.075)" fill="none" stroke="#ffd88a" stroke-width="0.6"/>
                  </svg>
                {% endif %}
              {% endfor %}
            </div>
          </div>
          
        </div>
        <div class="description-nutri">
          <p>{{ block.settings.description }}</p>
        </div>
      </div>
    </div>
  {% endfor %}
  </div>
  <div class="swiper-pagination"></div>
</div>
{% if section.settings.borda-inferior %}
  {% render 'borda-inferior', cor: section.settings.color-borda-inferior %}
{% endif %}
{% schema %}
  {
    "name": "Nutricionistas",
    "class": "blue-section",
    "settings": [{
      "type": "checkbox",
      "id": "borda-superior",
      "default": true,
      "label": "Ativar divisória superior"
    },
    {
      "type": "color",
      "id": "color-borda-superior",
      "default": "#fff",
      "label": "Cor da divisória superior"
    },
    {
      "type": "checkbox",
      "id": "borda-inferior",
      "default": false,
      "label": "Ativar divisória inferior"
    },
    {
      "type": "color",
      "id": "color-borda-inferior",
      "default": "#fff",
      "label": "Cor da divisória inferior"
    }],
    "blocks": [{
      "type": "Nutricionista",
      "name": "Nutricionista",
      "settings": [
        {
          "type": "image_picker",
          "id": "icone",
          "label": "Icone"
        },
        {
          "type": "range",
          "id": "star_range",
          "label": "Estrelas",
          "min": 0,
          "max": 5,
          "step": 0.5,
          "unit": "✦",
          "default": 5
        },
        {
          "type": "text",
          "id": "title",
          "label": "Título"
        },
        {
          "type": "textarea",
          "id": "description",
          "label": "Descrição"
        }
      ]
    }],
    "presets": [
      {"name": "Nutricionistas"}
    ]
  }
{% endschema %}