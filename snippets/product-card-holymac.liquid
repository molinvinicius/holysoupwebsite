        <img
          loading="lazy"
          width="100%"
          height="100%"
          src="{{ product.featured_media | image_url: width: product.featured_media.width }}"
        >
      {% else %}
        <img loading="lazy" width="100%" height="100%" src="{{ image | image_url: width: image.width }}">
      {% endif %}
