<style>
  .product-card-item {
    border-radius: 36px;
    display: flex;
    flex-direction: column;
  }

  .product-item-body {
    padding: 16px 20px;
    display: flex;
    flex-direction: column;
    gap: 16px;
    flex: 1;
  }

  .product-item-body .product-price {
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
  }

  .product-item-body .product-price-container {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
    gap: 12px;
    margin-top: auto;
  }

  .product-item-body .cta-container {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
  }

  .product-item-body .product-title {
    font-size: 15px;
    line-height: 24px;
    font-weight: bold;
  }

  .product-item-body .product-subtitle {
    font-size: 15px;
    line-height: 24px;
    font-weight: bold;
  }

  .product-item-body .product-description p {
    font-size: 12px;
    margin: 8px 0 0;
    line-height: normal;
    color: #231F20;
  }

  .product-card-item .product-header {
    display: flex;
    flex-direction: column;
    gap: 16px;
  }

  .product-card-item  .product-header img {
    object-fit: contain;
    width: 100%;
  }

  .cta-container {
    gap: 12px;
  }

  .cta-container .quantity-container input {
    width: 100%;
    max-width: 100px;
  }

  .cta-container .btn {
    text-transform: uppercase;
    font-weight: bold;
    font-size: 10px;
    letter-spacing: 0.5px;
    padding: 16px 44px;
    box-shadow: 0px 6px 6px #00000029;
    border-radius: 24px;
    min-width: auto;
    height: auto;
  }

  new.font-3 {
    color: #000;
    font-size: 19px;
    font-weight: bold;
  }

  old.custom-font {
    font-weight: 400;
    text-decoration: line-through;
    font-size: 13px;
    margin-bottom: 3px;
    color: #5A5A5A;
  }

  .portion-icon-container {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background: #02B456;
    padding: 4px;
    width: 32px;
    height: 32px;
    border-radius: 9999px;
  }

  svg.portion-icon {
    color: #fff;
    width: 28px;
    height: auto;
  }

  .portion-text-container {
    color: #231F20;
    font-size: 10px;
    display: flex;
    flex-direction: column;
    line-height: normal;
  }

  .portion-container {
    display: flex;
    align-items: center;
    gap: 10px;
  }

  .portion-value {
    font-size: 10px;
    display: flex;
    align-items: center;
    gap: 4px;
    font-weight: 500;
  }

  .portion-value b {
    color: #02B456;
  }

  .tabela-nutricional a {
    text-decoration: underline;
    font-size: 10px;
  }

  .tabela-nutricional a svg path {
    stroke: #00814e;
  }

  .product-card-item .modal-button {
    cursor: pointer;
    width: fit-content;
  }

  .product-card-item .modal-button svg {
    width: 125px;
    height: auto;
  }

  .cupom {
    color: #F6475A;
    font-size: 10px;
    text-align: center;
  }

  .cupom u {
    font-weight: bold;
    text-transform: uppercase;
  }

  .quantity-container input {
    color: #8355A7;
    font-size: 25px;
    font-weight: bold;
    border: 0;
    background: #fff;
  }

  .previewCartItem-qty .btn-quantity:before {
    height: 2px!important;
    background: #8355A7!important;
  }

  .previewCartItem-qty .btn-quantity {
    margin: 0 8px;
    padding: 0;
  }

  .previewCartItem-qty .btn-quantity.plus:after {
    height: 2px!important;
    background: #8355A7!important;
  }

  .product-card-item .product-infos {
    display: flex;
    gap: 8px;
    padding: 0 20px;
  }

  .product-card-item .product-infos svg {
    width: auto;
    height: auto;
    stroke-width: 0.1;
  }

  .product-card-item .product-infos .infos {
    display: flex;
    gap: 2px;
    color: #949494;
    font-size: 6px;
    align-items: center;
  }

  .product-card-item .product-infos span {
    font-size: 12px;
    font-weight: bold;
    white-space: nowrap;
  }
</style>
