    </div>
    <div>
      <a href="{{ product.url }}" alt="{{ product.title }}" class="product-title font-3">
        {% if block.settings.show_product_title %}
          {{ product.title }}
        {% else %}
          {{ block.settings.title }}
        {% endif %}
      </a>
      <div class="product-description">
        {% if block.settings.show_product_description %}
          {{ product.description }}
        {% else %}
          {{ block.settings.description }}
        {% endif %}
      </div>
    </div>
    <div class="product-price-container">
      <div class="product-price">
        {% if product.compare_at_price > product.price -%}
          <old class="custom-font">{{ product.compare_at_price | money }}</old>
        {%- endif %}
        <new class="font-3">{{ product.price | money }}</new>
      </div>
      <div class="portion-container">
        <div class="portion-icon-container" style="--background: {{ block.settings.color }}">
          <svg
            class="portion-icon"
            width="58"
            height="46"
            viewBox="0 0 58 46"
            fill="none"
            xmlns="http://www.w3.org/2000/svg"
          >
